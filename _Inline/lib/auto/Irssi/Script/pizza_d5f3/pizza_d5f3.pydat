def pizza():
    import random
    
    def random_line(afile):
        line = next(afile)
        for num, aline in enumerate(afile):
            if random.randrange(num + 2): continue
            line = aline
        return line

    noun_file = open('/Users/greg/.irssi/scripts/nouns.txt', 'r')
    pizza = "pizza %s" % str(random_line(noun_file)).rstrip()
    noun_file.close()
    return pizza

